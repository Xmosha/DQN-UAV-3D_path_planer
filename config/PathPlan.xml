<simulator>
    <env>
        <Env_Type>PathPlan_Env</Env_Type>
        <len>500</len>
        <width>500</width>
        <h>1</h>
        <eps>0.1</eps>
        <Is_On_Policy>0</Is_On_Policy>
        <FL_Loop>10</FL_Loop>
        <print_loop>2</print_loop>
        <num_UAV>1</num_UAV>
        <grid_len>1</grid_len>
        <Agent>
            <xml_path_agent>./config/ZDJ.xml</xml_path_agent>
           <Trainer>
                <Trainer_path>./config/Trainer.xml</Trainer_path>
            </Trainer>
        </Agent>

        <Threatens>
            <Threatens_path>./config/Threatens.xml</Threatens_path>
        </Threatens>

        <UI>
            <UI_path>./config/UI.xml</UI_path>
        </UI>
        <DB>
            <DB_path>./config/DB.xml</DB_path>
        </DB>
    </env>
    <is_prun>1</is_prun>
    <after_prun_size>30</after_prun_size>
    <record_epo>10</record_epo>
    <num_episodes>500</num_episodes> 
    <max_eps_episode>1000</max_eps_episode>
    <min_eps>0.1</min_eps>
    <TARGET_UPDATE>3</TARGET_UPDATE>
</simulator>